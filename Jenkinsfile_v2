pipeline {
    agent any
    
    
    stages {
        stage('Build'){
            steps {
                sh 'mvn clean package'
            }
        }
        
        stage('Test'){
            steps {
               sh 'mvn test'
               sh 'mvn clean install -Dtestfx.robot=glass -Dglass.platform=Monocle -Dmonocle.platform=Headless -Dprism.order=sw'
            }
        }
    }
}
